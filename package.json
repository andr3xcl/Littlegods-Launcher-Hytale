    "react-dom": "^18.2.0",
    "tar": "^7.5.3",
    "three": "^0.182.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@tabler/icons-react": "^3.36.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/discord-rpc": "^4.0.10",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/tar": "^6.1.13",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
