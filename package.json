{
  "name": "littlegods-launcher",
  "description": "The best Hytale Launcher by LittleGods",
  "author": "Littlegods <contact@littlegods.com>",
  "homepage": "https://github.com/andr3xcl/Littlegods-Launcher-Hytale",
